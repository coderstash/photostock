.logout {width: 60px; top: 8px;}
.circle_close{background:url(/images/close.png) 0px 0px no-repeat;text-indent:-9999em;}
.logout_button {padding: 0px;height: 28px;width: 30px;position: absolute;right: 20px;top: 10px;}
.internal_logout_button {position: absolute;top: 2px;border-radius: 0px;height: 30px;width: 30px;padding: 0px;right: 6px;}
.toolbar, .toolbar ul {position: relative; float: right;}
.toolbar_button {position: relative; width: 30px;height: 30px;padding: 0;float: left;margin: 0px 0px 0px 10px; list-style-type: none;}
.toolbar_button:hover {background:#F2F6FD;}

.field .button {width: 175px;}

.toggles span {color: #999; padding: 0px; position: relative;}
.toggles span:hover {cursor: pointer; color: #0088cc;}
.toggles span.selected{background: #1A7BA4; color:#fff;padding: 2px 10px;border-radius: 16px;}
.counter_toggles.toggles span, .counter_toggles.toggles span.selected{padding: 2px 21px 2px 10px;}
.counter_toggles span.badge {position: absolute;width: 11px;height: 11px;color: #FFF;padding: 3px 0px 3px 5px;font-size: 11px;border-radius: 8px;}

.listed_item.active {color:#fff;}
#msmapp .toolbar_button {width: 20px; height: 16px;}


