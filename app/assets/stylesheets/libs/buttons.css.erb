/* Buttons */
.button{position:relative;display:block;outline:none;cursor:pointer;text-align:center;text-decoration:none;font:14px/100% Arial,Helvetica,sans-serif;padding:10px 25px;text-shadow:0 1px 1px rgba(0,0,0,.3);-webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px; box-shadow:0 0px 5px rgba(0,0,0,.4);color:#fff;}
.button:hover{text-decoration:none;color:#fff;}
.large_button{padding:15px 45px;font-size:20px;color:white;text-shadow:0px 0px 5px #F9AD0B;font-weight:500; box-shadow:0px 0px 10px rgba(0,0,0,0.6);}
.red_button{background:#a80303;/* Old browsers */
background:-moz-linear-gradient(top,#a80303 0%,#7c0000 100%);/* FF3.6+ */
